{
  "name": "njct",
  "version": "1.0.0",
  "description": "Inject a dependency",
  "main": "lib",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test": "npm run t",
    "clean": "rm -rf lib",
    "pretest": "npm run build",
    "build": "npm run clean && npm run compile",
    "compile": "tsc",
    "compile:w": "tsc -w",
    "t": "npm run mocha -- src/*.spec.ts",
    "test:w": "npm run mocha -- --watch src/*.spec.ts ",
    "dev": "npm run compile && concurrently -r -k -p \"{name}:\" -n \"c,t\" \"npm run compile:w\" \"npm run test:w\"",
    "mocha": "ts-node -F node_modules/mocha/bin/_mocha",
    "release": "npm run build && np"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlight/inject.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "injector",
    "inject",
    "dependency",
    "di"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unlight/inject/issues"
  },
  "homepage": "https://github.com/unlight/inject#readme",
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.0.26",
    "concurrently": "^3.5.0",
    "mocha": "^3.5.0",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.2"
  }
}
